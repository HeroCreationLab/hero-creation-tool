    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chalk": "^4.1.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^9.1.0",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rollup": "^2.45.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.32.11",
    "semver": "^7.3.5",
    "ts-jest": "^26.5.5",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "yargs": "^16.2.0"
  },
  "lint-staged": {
    "*.(ts|js)": "eslint --fix",
